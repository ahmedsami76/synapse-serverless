			"defaultValue": "synapsebq5d8yi"
		},
		"synapsebq5d8yi-WorkspaceDefaultSqlServer_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'synapsebq5d8yi-WorkspaceDefaultSqlServer'"
		},
		"synapsebq5d8yi-WorkspaceDefaultStorage_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://datalakebq5d8yi.dfs.core.windows.net"
